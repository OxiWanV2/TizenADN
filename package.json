{
  "name": "@oxiwan/tizenadn",
  "appName": "ADN - Anime Digital Network",
  "version": "1.3.0",
  "description": "TizenBrew module pour Anime Digital Network sur TV Samsung",
  "packageType": "mods",
  "websiteURL": "https://animationdigitalnetwork.fr",
  "main": "dist/userScript.js",
  "serviceFile": "dist/service.js",
  "scripts": {
    "install-deps": "cd mods && npm install && cd ../service && npm install && cd ..",
    "build": "cd mods && npm run build && cd ../service && npm run build && cd .."
  },
  "keys": [
    "MediaPlayPause",
    "MediaPlay",
    "MediaPause",
    "MediaStop",
    "MediaFastForward",
    "MediaRewind",
    "MediaTrackNext",
    "MediaTrackPrevious"
  ],
  "author": "Lorenzo",
  "license": "MIT",
  "keywords": ["tizenbrew", "tizen", "samsung", "tv", "adn", "anime"],
  "repository": {
    "type": "git",
    "url": "https://github.com/OxiWanV2/TizenADN"
  },
  "publishConfig": {
    "access": "public"
  }
}